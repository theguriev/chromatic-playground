{
    "name": "@react-twitter/ui",
    "version": "0.1.0",
    "description": "Everything related to UI.",
    "keywords": [
        "canvas",
        "react",
        "hooks"
    ],
    "main": "dist/index.js",
    "scripts": {
        "build": "node build.js",
        "start": "start-storybook -p 6006 --no-manager-cache",
        "watch": "watch 'npm run build' src",
        "lint": "npx eslint --fix --ignore-path ../../.gitignore .",
        "test": "jest",
        "test:watch": "jest --watchAll"
    },
    "author": "Eugen Guriev",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "babel": {
        "plugins": [
            [
                "module-resolver",
                {
                    "root": [
                        "./src"
                    ],
                    "alias": {
                        "@": "./src"
                    }
                }
            ],
            "dynamic-import-node",
            "add-module-exports",
            "babel-plugin-react-scoped-css"
        ],
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "chrome": 78,
                        "edge": 15,
                        "esmodules": false
                    }
                }
            ],
            "@babel/preset-react"
        ]
    },
    "devDependencies": {
        "@storybook/addon-actions": "^6.1.21",
        "@storybook/addon-essentials": "^6.1.21",
        "@storybook/addon-links": "^6.1.21",
        "@storybook/react": "^6.1.21",
        "babel-plugin-add-module-exports": "^1.0.4",
        "babel-plugin-module-resolver": "^4.1.0"
    },
    "peerDependencies": {
        "lodash-es": "^4.17.21",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.5",
        "react-use": "^17.3.1"
    },
    "bugs": {
        "url": "https://github.com/theguriev/react-mind-map/issues"
    },
    "homepage": "https://github.com/theguriev/react-mind-map",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/theguriev/react-mind-map.git"
    },
    "dependencies": {
        "babel-plugin-react-scoped-css": "^1.1.1"
    }
}
